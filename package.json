{
  "name": "design-system-react",
  "version": "1.0.0",
  "description": "A Design system build with react and storybook",
  "main": "index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf build",
    "build": "npm run build:comp && npm run build:sb",
    "build:comp": "rollup -c",
    "build:sb": "build-storybook",
    "check:errors": "tsc --noEmit && npm run lint && npm run test",
    "check:setup": "npm run build && npm run test && npm run build-storybook",
    "lint:style": "stylelint '**/*.scss'",
    "lint:style:fix": "stylelint '**/*.scss' --fix",
    "lint:script": "eslint '*/**/*.{js,jsx,ts,tsx}'",
    "lint:script:fix": "eslint '*/**/*.{js,jsx,ts,tsx}' --fix",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:fix": "npm run lint:script:fix && npm run lint:style:fix",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006"
  },
  "repository": {
    "type": "git",
